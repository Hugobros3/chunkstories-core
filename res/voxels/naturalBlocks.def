/* Dirt with grass on top */
voxel grass {
	textures {
		top: ~_top
		sides: ~_side
	}
}

voxel forestgrass {
	textures {
		top: ~_top
		sides: ~_side
	}
}

voxel drygrass {
	textures {
		top: ~_top
		sides: ~_side
	}
}

/** Grass blades */
voxel grass_prop {
	solid: false
	opaque: false
	representation {
		model: ./voxels/models/grass.obj
	}
}

voxel dead_grass {
	solid: false
	opaque: false
	representation {
		model: ./voxels/models/grass.obj
	}
}

/** Trunk woods */
voxel wood {
	material: bluntwood
	textures {
		top: ~_top
		bottom: ~_top
	}
}

voxel pinewood {
	material: bluntwood
	textures {
		top: ~_top
		bottom: ~_top
	}
}

voxel birchwood {
	material: bluntwood
	textures {
		top: ~_top
		bottom: ~_top
	}
}

voxel leaves {
	class: io.xol.chunkstories.core.voxel.VoxelLeavesLod
	opaque: false
